.case{
  margin-bottom: 18rpx; 
}
.case_r{
  margin-top: 40rpx;
}
.case_list{
  margin-bottom: 40rpx;
}
.case_list .img_box{
  position: relative;
  width: 100%;
  height: 267rpx;
  margin-bottom: 26rpx;
}
.case_list .img_box image{
  width: 100%;
