}

.case_list .btn{
  width: 408rpx;
  height: 46rpx;
  border: 1px solid #dedede;
  border-radius: 23rpx;
  font-size: 20rpx;
  color: #676767;
  text-align: center;
  line-height: 46rpx;
  margin: 26rpx auto;
}