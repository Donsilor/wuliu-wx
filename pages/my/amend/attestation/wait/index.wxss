/* pages/my/amend/attestation/wait/index.wxss */
page{
  background-color: #fff;
}
.container{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 130rpx 30rpx 0;
  box-sizing: border-box;
}
.icon{
  width: 200rpx;
  height: 200rpx;
  background: url('https://szdbi.oss-cn-shenzhen.aliyuncs.com/guojiwuliu/home/icon_success%402x.png') no-repeat center;
  background-size: 100% 100%;
}
.tit{
  font-size: 34rpx;
  color: #333;
  margin-top: 30rpx;
}
.status{
  font-size: 28rpx;
  color: #999;
  margin-top: 20rpx;
}
.order_numb{
  font-size: 24rpx;
  color: #999;
  margin-top: 60rpx;
}
.finish{
  width: 100%;
  height: 80rpx;
  background-color: #ff6305;
  border-radius: 5rpx;
  font-size: 31rpx;
  color: #f3f3f3;
  text-align: center;
  line-height: 80rpx;
  margin-top: 120rpx;
}
