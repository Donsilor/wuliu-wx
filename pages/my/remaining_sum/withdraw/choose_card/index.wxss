/* pages/my/remaining_sum/withdraw/choose_card/index.wxss */
.container{
  padding: 0 30rpx 100rpx;
  box-sizing: border-box;
}
.tit{
  height: 76rpx;
  font-size: 28rpx;
  color: #999;
  line-height: 84rpx;
}
.card_list{
  height: 150rpx;
  background-color: #fff;
  border-radius: 5rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}
.card_list .img{
  width: 70rpx;
  height: 70rpx;
  background: url('https://szdbi.oss-cn-shenzhen.aliyuncs.com/guojiwuliu/icon_bankcard%402x.png') no-repeat center;
  background-size: 100% 100%;
}
.card_list .right{
  flex: 1;
  height: 100rpx;
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.type{
  font-size: 34rpx;
  color: #333;
}
.user{
  font-size: 26rpx;
  color: #888
}
.numb>span{
  margin-left: 16rpx;
}

.btn{
  position: fixed;
  left: 0;
  width: 100%;
  height: 98rpx;
  background-color: #ff6305;
  bottom: 0;
  text-align: center;
  line-height: 98rpx;
  font-size: 35rpx;
  color: #f3f3f3;
}